# -- Filesystem --
alias lh='ls -A'
alias tree2='tree -L 2'

# -- git --
alias gha='cat ~/.oh-my-zsh/plugins/git/git.plugin.zsh | grep'

# -- Shell --
alias path='echo ${PATH//:/\\n}'
alias reload='clear && exec $SHELL -l'

# -- tmux --
alias t='tmux'
alias tksv='tmux kill-server'
alias tls='tmux list-sessions'
